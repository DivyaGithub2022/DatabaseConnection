package Collections;

import java.util.Stack;

public class StackExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Stack<Integer> stack1 = new Stack<Integer>();
		Stack<Integer> stack1 = new Stack<>();
		
		stack1.push(4);
		stack1.push(5);
		stack1.push(14);
		stack1.push(12);
		
		System.out.println(stack1);
		
		stack1.pop();//to remove the last_in value in stack
		stack1.push(10);
		
		System.out.println(stack1);
		
		System.out.println(stack1.peek());//to get the value last entered in stack
		System.out.println(stack1.firstElement()); //What is VECTOR ?
		//to print first element but not of stack but Vector
		
	}

}
